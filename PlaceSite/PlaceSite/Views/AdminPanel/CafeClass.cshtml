@using PlaceSite.Models;
@model List<CafeClass>
@{
    ViewBag.Title = "CafeClass";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Class Name</th>

    </tr>
    @foreach (var u in Model)
    {
        <tr>
            <td>@u.Id</td>
            <td>@u.Type</td>
            <td> <a href="/AdminPanel/CafeClassDelete/@u.Id" class="btn btn-danger">Delete</a></td>
            <td> <a href="/AdminPanel/CafeClassUpgrade/@u.Id" class="btn btn-success">Update</a></td>
        </tr>
    }
</table>
<a href="/AdminPanel/CafeClassAdd/" class="btn btn-info">Add To Cafe</a>

