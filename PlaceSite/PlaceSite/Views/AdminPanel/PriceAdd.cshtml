@model PlaceSite.Models.Price
@{
    ViewBag.Title = "PriceAdd";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Add To Price</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.RoomName);
    @Html.TextBoxFor(x => x.RoomName, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.RoomPrice);
    @Html.TextBoxFor(x => x.RoomPrice, new { @class = "form-control" })
    <br />

    <button class="btn btn-primary">Save</button>
</form>
